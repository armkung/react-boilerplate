{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node server/server.js",
    "start:dashboard": "webpack-dashboard node server/server.js",
    "build": "webpack --config webpack/webpack.prod.config.js",
    "test": "BABEL_ENV=test jest",
    "lint": "eslint src --ext .js*"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/__test__/**.spec.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^(?!.*\\.(js|json)$)": "jest-file"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.js",
      "jest-canvas-mock"
    ],
    "moduleDirectories": [
      "<rootDir>/src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "react-native": "<rootDir>/node_modules/react-native-web",
      "styled-components/native": "<rootDir>/node_modules/styled-components"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!react-router-native)"
    ]
  },
  "dependencies": {
    "@redux-offline/redux-offline": "^2.3.2",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.2.8",
    "apollo-link-http": "^1.5.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "lodash": "^4.17.4",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mobx-state-tree": "^1.3.1",
    "preload-webpack-plugin": "https://github.com/GoogleChromeLabs/preload-webpack-plugin.git",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-native-web": "^0.5.3",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-native": "^4.2.0",
    "recompose": "^0.25.1",
    "redux": "^3.6.0",
    "redux-persist": "^4",
    "redux-saga": "^0.15.3",
    "reselect": "^3.0.1",
    "styled-components": "^3.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/polyfill": "^7.0.0-0",
    "@babel/preset-env": "^7.0.0-0",
    "@babel/preset-react": "^7.0.0-0",
    "@babel/preset-stage-0": "^7.0.0-0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^22.4.1",
    "babel-loader": "^8.0.0-0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-react-native-web": "^0.5.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react-app": "^4.0.0-0",
    "babel-preset-react-native": "^4.0.0",
    "clean-webpack-plugin": "^0.1.18",
    "compression": "^1.7.0",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.4.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.6.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^4.0.0-0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^22.2.2",
    "jest-canvas-mock": "^1.0.2",
    "jest-fetch-mock": "^1.5.0",
    "jest-file": "^1.0.0",
    "jest-in-case": "^1.0.2",
    "jest-styled-components": "^5.0.0",
    "jscodeshift": "^0.4.0",
    "lodash-webpack-plugin": "^0.11.4",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "postcss": "^6.0.8",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-nested": "^2.1.0",
    "postcss-url": "^7.1.1",
    "react-hot-loader": "^3.0.0",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.0.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.9",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-dev-server": "^3.0.0",
    "webpack-hot-middleware": "^2.21.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
