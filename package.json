{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node server/server.js",
    "start:dashboard": "webpack-dashboard node server/server.js",
    "build": "yarn clean; webpack -p --config webpack/webpack.prod.config.js",
    "test": "jest",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .js*"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/__test__/**.spec.js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^(?!.*\\.(js|json)$)": "jest-file"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleDirectories": [
      "<rootDir>/src",
      "node_modules"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/polyfill": "^7.0.0-0",
    "@babel/preset-env": "^7.0.0-0",
    "@babel/preset-react": "^7.0.0-0",
    "@babel/preset-stage-0": "^7.0.0-0",
    "babel-core": "^7.0.0-0",
    "babel-loader": "^8.0.0-0",
    "babel-plugin-lodash": "^3.3.2",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mobx-state-tree": "^1.3.1",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.8",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-nested": "^2.1.0",
    "postcss-url": "^7.1.1",
    "preload-webpack-plugin": "^2.2.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "recompose": "^0.25.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.2.1",
    "redux-saga": "^0.15.3",
    "reselect": "^3.0.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^22.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "compression": "^1.7.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.4.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.6.1",
    "express": "^4.15.2",
    "jest": "^22.2.2",
    "jest-file": "^1.0.0",
    "jest-in-case": "^1.0.2",
    "jscodeshift": "^0.4.0",
    "morgan": "^1.8.1",
    "null-loader": "^0.1.1",
    "react-test-renderer": "^16.0.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
